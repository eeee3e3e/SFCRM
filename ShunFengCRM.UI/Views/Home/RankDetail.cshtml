    <script type="text/javascript">
        function BindingText()
        {
            SimulationFormRequest();
        }

        function SimulationFormRequest()
        {
            
            var data =
                {
                    strPosition: localStorage.getItem("strPosition")
                }

            $.post("/Home/RankByLevel", data, function (returnData) {
                if (returnData.ReturnTypeStr == "Success") {
                    $("#divContent").html(returnData.Data);
                    return true;
                }
                else {
                    alert("数据加载有误");
                    alert(returnData.WarnMessage);
                    return false;
                }

            })
        }

    </script>
